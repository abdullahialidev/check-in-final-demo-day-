
<body id="top">
  <head>
    <meta charset="utf-8">
    <title>Student Profile</title>
    <%- include('partials/header') -%>
    <nav class="navbar navbar-expand-lg navigation" id="navbar">
    <div class="container">
      <a class="navbar-brand" href="index.html">
        <img src="imgs/logo.png" alt="" class="img-fluid">
      </a>
  
      <button class="navbar-toggler collapsed" type="button" data-toggle="collapse" data-target="#navbarmain"
        aria-controls="navbarmain" aria-expanded="false" aria-label="Toggle navigation">
        <span class="icofont-navigation-menu"></span>
      </button>
  
      <div class="collapse navbar-collapse" id="navbarmain">
        <ul class="navbar-nav ml-auto">
          <li class="nav-item active"><a class="nav-link" href="/profile">Profile</a></li>
          <li class="nav-item"><a class="nav-link" href="/mood">Mood</a></li>
          <li class="nav-item"><a class="nav-link" href="/chat">Chat</a></li>
          <li class="nav-item"><a class="nav-link" href="/logout">Logout</a></li>
        </ul>
      </div>
    </div>
  </nav>
  </head>  
  <section class="section student-single">
    <div class="container">
      <div class="row">

        <div class="student-img-block">
          <h4 class="mb-0"><%= studentName %></h4>
          <img src="imgs/empty.jpg" alt="" class="img-fluid w-100">
          <div class="info-block mt-4">
            <ul class="list-inline mt-4 student-social-links">
              <li class="list-inline-item"><a href="#!"><i class="icofont-facebook"></i></a></li>
              <li class="list-inline-item"><a href="#!"><i class="icofont-twitter"></i></a></li>
              <li class="list-inline-item"><a href="#!"><i class="icofont-skype"></i></a></li>
              <li class="list-inline-item"><a href="#!"><i class="icofont-linkedin"></i></a></li>
              <li class="list-inline-item"><a href="#!"><i class="icofont-pinterest"></i></a></li>
            </ul>
          </div>
        </div>
        <% for (let i = 0; i < submissions.length; i++) {%>
          <div class="col-lg-4 col-md-6">

          </div>
          <p><%= submissions[i].entryDate%></p>
        <div class="col-lg-8 col-md-6">
          <div class="student-details mt-4 mt-lg-0">
            <h2 class="text-md">Mood: <%= submissions[i].mood%></h2>
            <div class="divider my-4"></div>
            Grade: <%= submissions[i].grade%> </br>
            Message: <%= submissions[i].message%> </br>

              <form
              class="col-1"
              action="/post//deleteReview/<%= submissions[i].id %>?_method=POST"
              method="POST"
              >
              <button class="btn btn-main-2 btn-round-full mt-3">Delete<i
                class="icofont-simple-right ml-2  "></i></button>
            </form>
          </div>
        </div>
      <%}%>
      </div>
    </div>
  </section>
<%- include('partials/footer') -%>